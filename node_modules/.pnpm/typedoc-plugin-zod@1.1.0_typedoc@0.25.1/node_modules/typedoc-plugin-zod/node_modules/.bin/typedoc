#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/typedoc@0.25.1_typescript@5.1.6/node_modules/typedoc/bin/node_modules:/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/typedoc@0.25.1_typescript@5.1.6/node_modules/typedoc/node_modules:/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/typedoc@0.25.1_typescript@5.1.6/node_modules:/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/typedoc@0.25.1_typescript@5.1.6/node_modules/typedoc/bin/node_modules:/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/typedoc@0.25.1_typescript@5.1.6/node_modules/typedoc/node_modules:/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/typedoc@0.25.1_typescript@5.1.6/node_modules:/Users/kimhabork/Downloads/metadata/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typedoc@0.25.1_typescript@5.1.6/node_modules/typedoc/bin/typedoc" "$@"
else
  exec node  "$basedir/../../../../../typedoc@0.25.1_typescript@5.1.6/node_modules/typedoc/bin/typedoc" "$@"
fi
